f(n) = f(n-1) + (n + n-1 + n-2 .... 2 + 1) + 2(1+2+...(n-1)/2) + (n%2==0?n/2?0)
     = f(n-1) + n(n+1)/2 + 2((n-1)*(n+1)/8) + (n%2==0?n/2?0)
     
f(1)=1
f(2)=f(1) + 3*2/2 + 1*3/4 + 1
    =5
f(3)=f(2) + 3*4/2 + 2*4/4
    = 5 + 6 + 2
    = 13
f(4)=f(3) + 4*5/2 + 3*5/4 + 2
    = 13 + 10 + 2 +2
    = 27
f(5)=f(4) + 5*6/2 + 6*4/4 
    = 27 + 15 + 6
    = 48
f(6)= 48 + 21 + 6 + 3
    = 78
f(7)= 78 + 28 + 12
    = 118                                         
f(8)= 118 + 36 + 12 + 4
    = 170
f(9)= 170 + 45 + 20
    = 235
f(10)=235 + 55 + 20 + 5
     =315
     

