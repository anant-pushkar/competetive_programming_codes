#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<limits.h>
int debug=0;
typedef long long int lld;
int main(int argc , char **argv)
{
	if(argc>1 && strcmp(argv[1],"DEBUG")==0) debug=1;
	int T,t;scanf("%d",&T);
	float val,r;
	for(t=1;t<=T;++t){
		scanf("%f",&r);
		printf("Case %d: %0.2f\n",t,( (16*r*r-1)*0.25 + 0.5 ) );
	}
	return 0;
}
