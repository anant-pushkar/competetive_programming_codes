#include<stdio.h>
#include<stdlib.h>
#include<string.h>
#include<limits.h>
int debug=0;
typedef long long int lld;
int main(int argc , char **argv)
{
	if(argc>1 && strcmp(argv[1],"DEBUG")==0) debug=1;
	int t,p;scanf("%d",&t);
	lld n;
	while(t--){
		scanf("%lld %d",&n,&p);
		if(p==0)printf("Airborne wins.\n");
		else printf("Pagfloyd wins.\n");
	}
	return 0;
}
